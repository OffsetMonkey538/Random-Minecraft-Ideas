{
  "schemaVersion": 1,
  "id": "withermeal",
  "version": "${version}",
  "name": "Withered Bone Meal",
  "description": "Adds Withered Bone Meal, kills plants and turns flowers into Wither Roses.",
  "authors": [
    "OffsetMonkey538"
  ],
  "contact": {
    "sources": "https://github.com/OffsetMonkey538/Random-Minecraft-Ideas",
    "issues": "https://github.com/OffsetMonkey538/Random-Minecraft-Ideas/issues"
  },
  "license": "MIT",
  "icon": "assets/withermeal/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.offsetmonkey538.withermeal.WithermealMain"
    ]
  },
  "depends": {
    "fabricloader": "*",
    "fabric": "*",
    "minecraft": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "${discord_invite}"
      },
      "parent": {
        "id": "${root_id}",
        "name": "${root_name}",
        "description": "${root_description}",
        "icon": "${root_icon}"
      }
    }
  }
}
